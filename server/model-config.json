{
  "_meta": {
    "sources": [
      "loopback/common/models",
      "loopback/server/models",
      "../common/models",
      "./models"
    ],
    "mixins": [
      "loopback/common/mixins",
      "loopback/server/mixins",
      "../common/mixins",
      "./mixins"
    ]
  },
  "user": {
    "dataSource": "mongo"
  },
  "accessToken": {
    "dataSource": "mongo",
    "public": false
  },
  "ACL": {
    "dataSource": "mongo",
    "public": false
  },
  "RoleMapping": {
    "dataSource": "mongo",
    "public": false,
    "options": {
      "strictObjectIDCoercion": true
    }
  },
  "Role": {
    "dataSource": "mongo",
    "public": false
  },
  "EmergencyEvent": {
    "dataSource": "mongo",
    "public": true
  },
  "attachment": {
    "dataSource": "storage",
    "public": true
  },
  "userCredential": {
    "dataSource": "mongo",
    "public": false
  },
  "userIdentity": {
    "dataSource": "mongo",
    "public": false
  }
}
